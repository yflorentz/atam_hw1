.global _start

.section .text

  mov $result, %rax
  cmpl $1, (%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
Node1: 
	.quad 0
	.int 1
	.quad Node2
Node2: 
	.quad Node1
	.int 2
	.quad Node3
Node3: 
	.quad Node2
	.int 7
	.quad Node4
Node4: 
	.quad Node3
	.int 4
	.quad 0
node: Node3
result: .byte 0
